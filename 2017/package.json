{
  "name": "react-boston",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gh-pages": "^2.0.1",
    "grid-styled": "^4.1.0",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.10.2",
    "react": ">=16.3.3",
    "react-dom": ">=16.3.3",
    "react-ga": "^2.2.0",
    "react-google-maps": "^9.4.5",
    "react-icons": "^2.2.5",
    "react-scripts": "^2.1.4",
    "react-scroll": "^1.5.2",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "serialize-javascript": "^3.1.0",
    "mixin-deep": "^2.0.1",
    "set-value": "^2.0.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start-js": "react-scripts start",
